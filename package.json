{
  "name": "appn",
  "version": "0.1.1",
  "description": "A simple web component",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "sideEffects": true,
  "type": "module",
  "scripts": {
    "dist": "tsc",
    "dist:watch": "tsc --watch",
    "react": "tsx scripts/react-generator.ts",
    "clean": "rimraf dist bundle docs",
    "lint:all": "pnpm lint:lita && pnpm lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lita": "lit-analyzer",
    "lita:fix": "eslint 'src/**/*.ts'  --fix",
    "lita:watch": "node --watch ./node_modules/lit-analyzer/cli.mjs",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "-docs": "pnpm docs:clean && pnpm dist && pnpm analyze && pnpm bundle && pnpm docs:assets && pnpm 11ty",
    "build": "pnpm dist --noCheck && pnpm analyze && pnpm bundle && pnpm 11ty",
    "11ty": "eleventy --config=eleventy.config.ts",
    "11ty:watch": "node --watch ./node_modules/@11ty/eleventy/cmd.cjs  --config=eleventy.config.ts --watch --serve",
    "bundle": "rollup -c",
    "bundle:watch": "pnpm bundle --watch",
    "docs:assets": "node scripts/assets.ts",
    "docs:serve": "wds --root-dir=docs --node-resolve --watch",
    "analyze": "cem analyze --litelement --globs \"src/**/*.ts\"",
    "analyze:watch": "cem analyze --litelement --globs \"src/**/*.ts\" --watch",
    "serve": "wds --watch",
    "serve:prod": "MODE=prod pnpm serve",
    "test": "pnpm wtr:dev && pnpm wtr:prod",
    "wtr:dev": "wtr --files dist/**/*.test.js",
    "wtr:watch": "pnpm wtr:dev --watch",
    "wtr:prod": "pnpm wtr:dev --prod",
    "wtr:prod:watch": "pnpm wtr:prod --watch",
    "checksize": "rollup -c ; cat my-element.bundled.js | gzip -9 | wc -c ; rm my-element.bundled.js"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript",
    "lit"
  ],
  "author": "Gaubee",
  "license": "MIT",
  "files": [
    "README.md",
    "bundle",
    "dist"
  ],
  "dependencies": {
    "@gaubee/util": "^0.30.2",
    "@gaubee/web": "^0.8.0",
    "@gaubee/tc39-shim": "^0.3.3",
    "@lit/context": "^1.1.5",
    "@lit/react": "^1.0.7",
    "@lit/task": "^1.0.2",
    "@std/fmt": "npm:@jsr/std__fmt@^1.0.6",
    "big.js": "^7.0.1",
    "csstype": "^3.1.3",
    "culori": "^4.0.1",
    "idb": "^8.0.2",
    "lit": "^3.3.0",
    "signal-polyfill": "^0.2.2",
    "signal-utils": "^0.21.1",
    "ts-deepmerge": "^7.0.3",
    "ts-pattern": "^5.7.0",
    "tslib": "^2.8.1",
    "urlpattern-polyfill": "^10.0.0"
  },
  "pnpm": {
    "overrides": {
      "@types/chai": "^5.2.0",
      "playwright": "^1.15.1",
      "@11ty/recursive-copy": "^4.0.0",
      "eslint": "^9.24.0"
    },
    "patchedDependencies": {
      "@open-wc/testing": "patches/@open-wc__testing.patch",
      "prismjs": "patches/prismjs.patch",
      "@11ty/recursive-copy": "patches/@11ty__recursive-copy.patch",
      "minify-literals": "patches/minify-literals.patch",
      "idb": "patches/idb.patch",
      "@jackolope/lit-analyzer": "patches/@jackolope__lit-analyzer.patch"
    }
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@11ty/eleventy-plugin-vite": "^6.0.0",
    "@custom-elements-manifest/analyzer": "^0.10.4",
    "@open-wc/testing": "^4.0.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/chai": "^5.2.1",
    "@types/culori": "^4.0.0",
    "@types/dom-navigation": "^1.0.5",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@types/sinon": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@web/dev-server": "^0.4.6",
    "@web/dev-server-legacy": "^2.1.1",
    "@web/test-runner": "^0.20.1",
    "@web/test-runner-playwright": "^0.11.0",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "chai": "^5.2.0",
    "chokidar": "^4.0.3",
    "cross-env": "^7.0.3",
    "custom-element-solidjs-integration": "^1.9.0",
    "es-escape-html": "^0.1.1",
    "eslint": "^9.26.0",
    "eslint-plugin-lit": "^2.1.1",
    "jiti": "^2.4.2",
    "lit-analyzer": "npm:@jackolope/lit-analyzer@^3.1.2",
    "playwright": "^1.52.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prismjs": "^1.30.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.40.1",
    "rollup-plugin-minify-template-literals": "^1.1.7",
    "rollup-plugin-summary": "^3.0.1",
    "sinon": "^20.0.0",
    "ts-lit-plugin": "npm:@jackolope/ts-lit-plugin@^3.1.2",
    "tsx": "^4.19.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.31.1",
    "vite": "^6.3.4"
  },
  "peerDependencies": {
    "@types/dom-navigation": "^1.0.5",
    "react": ">=18"
  },
  "customElements": "custom-elements.json"
}

{
  "name": "appn",
  "version": "0.0.1",
  "description": "A web component library focuses on building a common Webapp navigation standard on the Web.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/appn/appn.esm.js",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "stencil build && pnpm script.reexports && pnpm format",
    "start": "stencil build --dev --watch --serve",
    "g": "stencil g",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "script.reexports": "node ./scripts/reexports.ts",
    "format": "prettier --write ."
  },
  "exports": {
    ".": {
      "import": "./dist/appn/appn.esm.js",
      "require": "./dist/appn/appn.cjs.js"
    },
    "./appn-footer": {
      "import": "./dist/components/appn-footer.js",
      "types": "./dist/components/appn-footer.d.ts"
    },
    "./appn-header": {
      "import": "./dist/components/appn-header.js",
      "types": "./dist/components/appn-header.d.ts"
    },
    "./appn-page": {
      "import": "./dist/components/appn-page.js",
      "types": "./dist/components/appn-page.d.ts"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gaubee/appn.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "devDependencies": {
    "@gaubee/nodekit": "^0.6.2",
    "@stencil/core": "^4.27.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "prettier": "^3.5.3",
    "puppeteer": "^24.3.0"
  },
  "license": "MIT",
  "dependencies": {
    "@gaubee/util": "^0.25.1"
  }
}
